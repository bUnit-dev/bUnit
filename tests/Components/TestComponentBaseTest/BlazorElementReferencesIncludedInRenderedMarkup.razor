@inherits TestComponentBase


<Fixture Test="Test">
    <Fragment>
        <div @ref="refElm" />
    </Fragment>
</Fixture>
@code {
    ElementReference refElm;

    void Test()
    {
        var cut = GetFragment();

        var html = cut.Markup;

        html.ShouldContain($"=\"{refElm.Id}\"");
    }
}